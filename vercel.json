{
  "version": 2,
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.js" },
    { "src": "/health", "dest": "/api/index.js" },
    { "src": "/(.*)", "dest": "/dist/public/$1" },
    { "src": "/", "dest": "/dist/public/index.html" }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
